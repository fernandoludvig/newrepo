<!-- **********************************
* Content Heading Section
* Displays the title as the main heading of the page
*********************************** -->
<% if (title) { %>
    <h1><%= title %></h1>
<% } else { %>
    <%- res.redirect('/') %> <!-- Redirects to the home page if there is no title -->
<% } %>

<!-- **********************************
* Message Section (Commented Out)
* Displays any messages or alerts (future implementation)
*********************************** -->
<%# messages() %> <!-- This code is commented out and will be enabled in the future -->

<!-- **********************************
* Inventory Items Display Section
* Displays the inventory grid, which contains vehicles information
*********************************** -->
<%- grid %> <!-- Displays the dynamically generated grid with HTML -->
